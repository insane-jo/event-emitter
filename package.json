{
  "name": "event-emitter-es6",
  "version": "1.1.5",
  "description": "Small event emitter for using in es6 and plain js",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test-helper.js --recursive",
    "build": "npm run bundle && npm run minify",
    "bundle": "node_modules/.bin/browserify index.js > dist/event-emitter.js -s EventEmitter -t [ babelify ] && echo \"bundled\"",
    "minify": "node_modules/.bin/uglifyjs dist/event-emitter.js -o dist/event-emitter.min.js -m && echo \"minified\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/insane-jo/event-emitter.git"
  },
  "keywords": [
    "eventemitter",
    "event-emitter",
    "event-emitter-es6"
  ],
  "author": "insane-jo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/insane-jo/event-emitter/issues"
  },
  "homepage": "https://github.com/insane-jo/event-emitter#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "uglify-js": "^2.6.2"
  }
}
